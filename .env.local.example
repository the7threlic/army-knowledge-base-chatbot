# OpenRouter API Configuration for AI-Powered Responses
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Default AI Model (optional - defaults to anthropic/claude-3.5-sonnet)
DEFAULT_AI_MODEL=anthropic/claude-3.5-sonnet

# SharePoint Configuration (Optional - uses simulation by default)
SHAREPOINT_SITE_URL=https://your-army-sharepoint.com
SHAREPOINT_CLIENT_ID=your_client_id
SHAREPOINT_CLIENT_SECRET=your_client_secret
SHAREPOINT_TENANT_ID=your_tenant_id
